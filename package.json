    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.9",
    "canvas-confetti": "^1.9.4",
    "dexie": "^3.2.4",
    "onnxruntime-web": "^1.23.2"
  }
}
